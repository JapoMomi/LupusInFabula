<div id="successMessage" class="message" style="display: none">
    <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
    <h4><strong id="successTitle" class="messageTitle"></strong></h4>
    <p id="successMessageDetails" class="messageDetails"></p>
    <button id="successShowMore" class="showMore mb-0" onclick="showMore('successMessage')">
        <span id="successShowMoreText" class="showMoreText m-auto">Show more</span>
    </button>
    <p id="successMoreDetails" class="moreDetails" style="display: none"></p>
</div>