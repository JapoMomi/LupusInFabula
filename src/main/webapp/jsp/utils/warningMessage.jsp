<div id="warningMessage" class="message" style="display: none">
    <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
    <h4><strong id="warningTitle" class="messageTitle"></strong></h4>
    <p id="warningMessageDetails" class="messageDetails"></p>
    <button id="warningShowMore" class="showMore mb-0" onclick="showMore('warningMessage')">
        <span id="warningShowMoreText" class="showMoreText m-auto">Show more</span>
    </button>
    <p id="warningMoreDetails" class="moreDetails" style="display: none"></p>
</div>